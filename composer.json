{
    "name": "ktzar/twitter_client",
    "description": "Show some tweets",
    "keywords": [
    ],
    "homepage": "http://github.com/ktzar/twitter_client",
    "license": "MIT",
    "authors": [
        {
            "name": "Miguel Gonzalez",
            "email": "miguelmail@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "monolog/monolog": "^2.0",
        "mustache/mustache": "^2.13",
        "php-di/php-di": "^6.1",
        "slim/psr7": "^1.1",
        "slim/slim": "^4.5",
        "vlucas/phpdotenv": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
